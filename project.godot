; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GGJStandupRPG"
run/main_scene="res://start_menu/StartMenu.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"

[autoload]

Types="*res://Types.gd"
NodeUtils="*res://util/NodeUtils.gd"
Events="*res://util/Events.gd"
GameState="*res://GameState.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Sfx="*res://sfx/Sfx.tscn"

[display]

window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/scene_manager/plugin.cfg")

[gui]

theme/custom="res://default_theme.tres"
theme/default_font_multichannel_signed_distance_field=true
